<% if use_caching? %>
  <%= cached_output.html_safe %>
<% else %>
  <%= tag.div(class: "panda-cms-content", **element_attrs) do %>
    <% unless @editable_state %>
      <%= @rendered_content.html_safe %>
    <% end %>
  <% end %>
<% end %>
<% if @footnotes&.any? %>
  <%= render partial: "panda/cms/footnotes", locals: { footnotes: @footnotes } %>
<% end %>
